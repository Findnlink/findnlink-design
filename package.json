{
  "name": "@findnlink/ui",
  "version": "0.2.33",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/ui.umd.js",
  "module": "./dist/ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/ui.es.js",
      "require": "./dist/ui.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "types": "./dist/index.d.ts",
  "private": false,
  "description": "Findnlink design system",
  "repository": "https://github.com/Findnlink/findnlink-design.git",
  "author": "<contact@findnlink.com>",
  "license": "MIT",
  "homepage": "https://www.findnlink.com",
  "keywords": [
    "Findnlink",
    "React",
    "Vite",
    "UI",
    "UX",
    "SCSS",
    "Storybook"
  ],
  "scripts": {
    "dev": "vite",
    "test": "jest",
    "build:types": "tsc src/index.ts --declaration --emitDeclarationOnly --jsx react --esModuleInterop --outDir dist",
    "build": "tsc && vite build && npm run build:types",
    "serve": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate": "node ./util/create-component",
    "publish": "npm publish --access public"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-jest": "^6.3.12",
    "@storybook/addon-links": "^6.4.0-beta.32",
    "@storybook/react": "^6.4.0-beta.32",
    "@testing-library/react": "^12.1.2",
    "@types/enzyme": "^3.10.10",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.0",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.0",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@types/testing-library__react": "^10.2.0",
    "@vitejs/plugin-react": "^1.0.8",
    "babel-loader": "^8.2.3",
    "babel-preset-react-app": "^10.0.0",
    "enzyme": "^3.11.0",
    "jest": "^27.3.1",
    "react": "^17.0.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-loading-skeleton": "^3.0.1",
    "sass": "^1.43.4",
    "sass-loader": "10.1.1",
    "storybook-builder-vite": "^0.1.6",
    "storybook-dark-mode": "^1.0.8",
    "tsup": "^5.6.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.4",
    "vite-dts": "^1.0.3"
  }
}
